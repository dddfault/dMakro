{
	"dMakro float variable with slider, input or drag option.": {
		"prefix": "mpfloat",
		"body": [
			"${1|MP_FLOAT_S,MP_FLOAT_I,MP_FLOAT_D|}(${2:varname},",
			"  \"${3:label}\",",
			"  \"${4:tooltip}\",",
			"  \"${5:text}\",",
			"  ${6:spacing}, ${7:minval}, ${8:maxval}, ${9:step}, ${10:defval})",
			"$0"
		],
		"description": "dMakro float variable with slider, input or drag option."
	},

